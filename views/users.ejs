<!DOCTYPE html>
<html>
<head>
  <title>Users</title>
  <style>
    .card {
      display: inline-block;
      width: 200px;
      margin: 10px;
      padding: 10px;
      border: 1px solid #ccc;
      border-radius: 8px;
      text-align: center;
    }
    img {
      width: 100%;
      height: auto;
      border-radius: 4px;
    }
  </style>
</head>
<body>
  <h1>Uploaded Users</h1>
  <a href="/">Go Back</a>
  <div style="display:flex; flex-wrap:wrap;">
    <% users.forEach(user => { %>
      <div class="card">
        <img src="<%= user.imageUrl %>" alt="User Image" />
        <h3><%= user.name %></h3>
        <p><%= user.phone %></p>
      </div>
    <% }) %>
  </div>
</body>
</html>
